ab(value)\n  }\n\n  const completeTodo = (key) => {\n    const newItems= [...items];\n    newItems[key].done = !newItems[key].done;\n    putItems(newItems);\n  };\n\n  return (\n    <div className=\"panel\">\n      <div className=\"panel-heading\">\n        ITSS ToDoアプリ\n      </div>\n      <Input addTask={addTask}/>\n      <Filter value={tab} changeTab={selectTab} />\n      {filteredItems.map((item, index) => (\n        <TodoItem \n          key={item.key}\n          item={item}\n          index={index}\n          completeTodo={completeTodo}\n        />\n      ))}\n      <div className=\"panel-block\">\n        {filteredItems.length} items\n      </div>\n        <div className=\"panel-block\">\n         <button className=\"button is-light is-fullwidth\" onClick={clearItems}>\n           全てのToDoを削除\n         </button>\n       </div>\n    </div>\n  );\n}\n\nexport default Todo;","/home/runner/ReactNguyenGiaMinh201841522/itss_std_react_todo_k63/src/components/TodoItem.js",["45"],"/* \n  【TodoItemコンポーネント】\n　・Todoアイテムを表示する\n　・チェックボックスにチェックが入っているか管理する\n　・チェックボックスにチェックが入っているかアイテムをグレーアウトする\n*/\nimport { useState } from 'react';\n\nfunction TodoItem({item, index, completeTodo}) {\n  return (\n    <label className=\"panel-block\">\n        <input \n          type=\"checkbox\" \n          checked={item.done}\n          onClick={() => completeTodo(index)}\n          />\n        <span class={item.done && \"has-text-grey-light\"}>{item.text}</span>\n    </label>\n  );\n}\n\nexport default TodoItem;","/home/runner/ReactNguyenGiaMinh201841522/itss_std_react_todo_k63/src/components/Filter.js",["46","47","48"],"/* \n  【Filterコンポーネント】\n　・該当するTodoをステータス毎にで分けてリスト表示する\n　・タブで表示する\n　・サポートするステータスは「すべて」「未完了」「完了済み」\n*/\nfunction Filter({ value, changeTab }) {\n  \n  const handleChangeFilter = (value, event) => {\n    event.preventDefault();\n    changeTab(value);\n  };\n  \n  return (\n    <div className=\"panel-tabs\">\n      <a href=\"#\" \n        className={value === 'ALL' && 'is-active'}\n        onClick={handleChangeFilter.bind(null, 'ALL')}  \n      >全て</a>\n      <a href=\"#\" \n        className={value === 'DOING' && 'is-active'}\n        onClick={handleChangeFilter.bind(null, 'DOING')}  \n      >未完了</a>\n      <a href=\"#\" \n        className={value === 'DONE' && 'is-active'}\n        onClick={handleChangeFilter.bind(null, 'DONE')}  \n      >完了済み</a>\n    </div>\n  );\n}\n\nexport default Filter","/home/runner/ReactNguyenGiaMinh201841522/itss_std_react_todo_k63/src/components/Input.js",[],"/home/runner/ReactNguyenGiaMinh201841522/itss_std_react_todo_k63/src/hooks/storage.js",[],"/home/runner/ReactNguyenGiaMinh201841522/itss_std_react_todo_k63/src/lib/util.js",[],{"ruleId":"49","replacedBy":"50"},{"ruleId":"51","replacedBy":"52"},{"ruleId":"53","severity":1,"message":"54","line":26,"column":44,"nodeType":"55","messageId":"56","endLine":26,"endColumn":46},{"ruleId":"57","severity":1,"message":"58","line":7,"column":10,"nodeType":"59","messageId":"60","endLine":7,"endColumn":18},{"ruleId":"61","severity":1,"message":"62","line":16,"column":7,"nodeType":"63","endLine":19,"endColumn":8},{"ruleId":"61","severity":1,"message":"62","line":20,"column":7,"nodeType":"63","endLine":23,"endColumn":8},{"ruleId":"61","severity":1,"message":"62","line":24,"column":7,"nodeType":"63","endLine":27,"endColumn":8},"no-native-reassign",["64"],"no-negated-in-lhs",["65"],"array-callback-return","Array.prototype.filter() expects a value to be returned at the end of arrow function.","ArrowFunctionExpression","expectedAtEnd","no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-